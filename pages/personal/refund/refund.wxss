
	.content{
		width: 100%;
		height: 100vh;
		background-color:#F5F5F5;
		padding-bottom: 100rpx;
	}
	.contentTitle{
		padding: 15rpx 30rpx;
		font-size: 30rpx;
		font-weight: bold;
	}
	.contentView{
		padding: 20rpx 30rpx;
		background-color: white;
		font-size: 26rpx;
	}
	.contentItem{
		display: flex;
		flex-direction: row;
		align-items: center;
		justify-content: space-between;
		margin-bottom: 20rpx;
	}
	.contentYuan{
		width: 40rpx;
		height: 40rpx;
		line-height: 40rpx;
		text-align: center;
		border-radius: 100%;
		border: 1px solid #DCDCDC;
		font-weight: bold;
		color: white;
	}
	.active1{
		border: 0;
		background-color: #72C8FF;
	}
	.red{
		color: red;
	}
	.bottom{
		width: 100%;
		height: 100rpx;
		line-height: 100rpx;
		text-align: center;
		color: white;
		font-size: 32rpx;
		font-weight: bold;
		background-color: #72C8FF;
		position: fixed;
		left: 0;
		bottom: 0;
	}
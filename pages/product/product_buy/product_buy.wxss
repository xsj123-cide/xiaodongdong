		/* z-index: 102; */
		.time{
			width: 100%;
			height: 1012rpx;
			padding: 20rpx 0;
			background-color: white;
			position: fixed;
			left: 0;
			bottom: 0;
			z-index: 999;
			border-top: 1px solid #F5F5F5;
		}
		.timeTile{
			padding: 0 30rpx;
			font-size: 30rpx;
			margin-bottom: 15rpx;
		}
		.timeMonth{
			padding: 0 30rpx;
			display: flex;
			flex-direction: row;
			align-items: center;
		}
		.timeItem1{
			font-size: 28rpx;
			font-weight: bold;
			letter-spacing: 2px;
			padding-bottom: 20rpx;
			margin-right: 50rpx;
			position: relative;
		}
		.monthActive{
		}
		.monthActive::after{
			content: "";
			width: 30rpx;
			border: 1px solid #71C7FF;
			position: absolute;
			bottom: 5rpx;
			left: 50%;
			transform: translateX(-50%);
		}
		.timeWeek{
			width: 100%;
			height: 50rpx;
			display: flex;
			flex-direction: row;
			background-color: #F3F3F3;
